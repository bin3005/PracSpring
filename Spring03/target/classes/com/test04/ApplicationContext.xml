<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="lee" class="com.test04.Emp">
		<property name="name" value="김김김"/>
		<property name="salary" value="3000000"/>
	</bean>

	<bean id="kim" class="com.test04.Emp">
		<property name="name" value="이이이"/>
		<property name="salary" value="2500000"/>
	</bean>


	<bean id="lee1" class="com.test04.Engineer">
		<constructor-arg index="0" ref="lee"/>
		<constructor-arg name="dept" value="수리부서"></constructor-arg>
	</bean>
	
	<bean id="kim1" class="com.test04.Developer">
		<constructor-arg index="0" ref="kim"></constructor-arg>
		<constructor-arg index="1" value="개발부서"/>
	
	</bean>
</beans>

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="lee" class="com.test03.Developer">
		<constructor-arg index="0" value="이예슬" />
		<constructor-arg index="1" value="2000000" />
		<property name="dept" value="개발" />
	</bean>
	
	<bean id="park" class="com.test03.Engineer">
		<constructor-arg index="0" value="박기민" />
		<constructor-arg index="1" value="3000000" />
		<property name="dept" value="기술" />
	</bean>
	
</beans>

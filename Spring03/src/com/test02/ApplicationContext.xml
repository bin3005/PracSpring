<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 생성자로 설정한 bean -->
	<bean id="lee" class="com.test02.Emp">
		<constructor-arg name="name" value="이창진" />
		<constructor-arg name="salary" value="1000000" />
	</bean>
	
	<!-- setter 메소드로 설정한 bean -->
	<bean id="moon" class="com.test02.Emp">
		<property name="name" value="문태연" />
		<property name="salary" value="2000000" />
	</bean>
</beans>

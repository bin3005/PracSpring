<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="engineer" class="com.test03.Engineer">
		<constructor-arg name="name" value="이승빈"></constructor-arg>
		<constructor-arg name="salary" value="2500000"></constructor-arg>
		<property name="dept" value="수리부서"></property>
	</bean>
	<bean id="developer" class="com.test03.Developer">
		<constructor-arg name="name" value="정처기"></constructor-arg>
		<constructor-arg name="salary" value="20000000"></constructor-arg>
		<property name="dept" value="개발부서"></property>
	</bean>
</beans>
